{
  "name": "edusanjal",
  "version": "1.0.0",
  "description": "Nepal's leading education portal",
  "author": "Awecode <info@awecode.com>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "NODE_ENV=production nuxt build",
    "start": "nuxt start",
    "generate": "NODE_ENV=production nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "e2e": "./node_modules/.bin/nightwatch tests/e2e",
    "test": "jest",
    "test:silent": "jest --silent",
    "test:coverage": "jest --coverage",
    "cypress": "cypress open"
  },
  "dependencies": {
    "@nuxtjs/pwa": "^2.0.8",
    "axios": "^0.18.0",
    "bulma": "^0.6.2",
    "nuxt": "^1.0.0",
    "vanilla-lazyload": "^8.7.1",
    "vuex-persistedstate": "^2.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.44",
    "@babel/polyfill": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@cypress/webpack-preprocessor": "^2.0.1",
    "@vue/server-test-utils": "^1.0.0-beta.13",
    "@vue/test-utils": "^1.0.0-beta.13",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.4.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "css-loader": "^0.28.11",
    "cypress": "^2.1.0",
    "cypress-vue-unit-test": "^1.11.0",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.0.0",
    "jest": "^22.4.3",
    "jest-serializer-vue": "^1.0.0",
    "nightwatch": "^0.9.20",
    "node-sass": "^4.8.3",
    "sass-loader": "^6.0.7",
    "sass-resources-loader": "^1.3.3",
    "vue-jest": "^2.4.1",
    "vue-loader": "^14.2.2",
    "vue-template-compiler": "^2.5.16"
  },
  "jest": {
    "globals": {
      "vue-jest": {
        "experimentalStyles": true
      }
    },
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ]
  }
}
